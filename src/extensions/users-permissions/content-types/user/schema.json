{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "nickname": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "group": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::group.group",
      "inversedBy": "users"
    },
    "line_id": {
      "type": "string"
    },
    "note": {
      "type": "text"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "main_point": {
      "type": "integer",
      "default": 0
    },
    "login_logs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::login-log.login-log",
      "mappedBy": "users"
    },
    "point_logs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::point-log.point-log",
      "mappedBy": "user"
    }
  }
}
